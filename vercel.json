{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --prefer-offline --no-audit",
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_API_BASE_URL": "@vite_api_base_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
